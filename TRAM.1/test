# VarChecker checks the use of variables:
# * the left-hand side contains at least one function symbol
# * every variable in the left-hand side of a rule occurs once
# * every variable in the right-hand side also occurs in the left-hand side
# Note that these are local concerns; global are only the program and the list of error messages
cat <<-'******************' > "VarChecker.trm"
	varchk(T) = mapvc(T);

	mapvc(trm(str('e',str('o',str('r',eos))),eoa)) = eol;
	mapvc(trm(Rl,arg(var(Nm),arg(Rhs,arg(Rls,eoa)))))	= vc2(var(Nm),eol,eol,Rhs,lst(err1(Nm),xmapvc(Rls)));
	mapvc(trm(Rl,arg(Lhs,arg(Rhs,arg(Rls,eoa))))) = vc2(Lhs,eol,eol,Rhs,mapvc(Rls));

	vc2(trm(F,As),Stck,Vars,Rhs,Errs) = vc2(As,Stck,Vars,Rhs,Errs);
	vc2(var(N),Stck,Vars,Rhs,Errs) = vc2q(eq(N,first(Vars)),rest(Vars),N,Stck,Vars,Rhs,Errs);
	vc2(arg(A,As),Stck,Vars,Rhs,Errs) = vc2(As,lst(A,Stck),Vars,Rhs,Errs);
	vc2(eoa,lst(X,Stck),Vars,Rhs,Errs) = vc2(X,Stck,Vars,Rhs,Errs);
	vc2(eoa,eol,Vars,Rhs,Errs) = vc3(Rhs,eol,Vars,Errs);

	vc2q(ok,Rest,N,Stck,Vars,Rhs,Errs) = vc2(eoa,Stck,Vars,Rhs,lst(err2(N),Errs));
	vc2q(*,lst(V,Rest),N,Stck,Vars,Rhs,Errs) = vc2q(eq(N,V),Rest,N,Stck,Vars,Rhs,Errs);
	vc2q(*,*,N,Stck,Vars,Rhs,Errs) = vc2(eoa,Stck,lst(N,Vars),Rhs,Errs);

	vc3(trm(F,As),Stck,Vars,Errs) = vc3(As,Stck,Vars,Errs);
	vc3(var(N),Stck,Vars,Errs) = vc3q(eq(N,first(Vars)),rest(Vars),N,Stck,Vars,Errs);
	vc3(arg(A,As),Stck,Vars,Errs) = vc3(As,lst(A,Stck),Vars,Errs);
	vc3(eoa,lst(X,Stck),Vars,Errs) = vc3(X,Stck,Vars,Errs);
	vc3(eoa,eol,Vars,Errs) = Errs;

	vc3q(ok,Rest,N,Stck,Vars,Errs) = vc3(eoa,Stck,Vars,Errs);
	vc3q(*,lst(V,Rest),N,Stck,Vars,Errs) = vc3q(eq(N,V),Rest,N,Stck,Vars,Errs);
	vc3q(*,*,N,Stck,Vars,Errs) = vc3(eoa,Stck,Vars,lst(err3(N),Errs));

	!X = f(Y); Note: unitary variable crashes the parser
	f(P,P) = Q;

******************

Test=410
Flags="-D 1 -X 20 -p ScannerParser.trm -p VarChecker.trm -p Lists.trm -p Strings.trm -C -s VarChecker.trm -M term1$Test.trm -r -O"
Doc="VarChecker check self"
cat <<-'******************' > "term1$Test.trm"
	varchk(scan(%1))
******************
cat <<-'******************' > "expect$Test.out"

	### Start ###
	### Done! ###
	lst(err3("Q"),lst(err2("P"),eol))
******************
testx "$Test" "$Doc" "$Flags"


