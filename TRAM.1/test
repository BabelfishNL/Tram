#!/bin/sh

if [ $# -lt 1 ]; then
    printf "Usage: %s <application>\n" "$0" >&2
    exit 1
fi
bin="./$1"           # The application (from command arg)
diff="diff -iad"   # Diff command, or what ever

test () {
	echo -n $2  '[' $bin -P "prog$1.trm" -T "term$1.trm" -r -O ']' "=>  "
	$bin -P "prog$1.trm" -T "term$1.trm" -r -O  > "out$1.out"
	$diff "out$1.out" "expect$1.out"
	e_code=$?
    if [ $e_code != 0 ]; then
            printf "TEST $1 FAIL : %d\n" "$e_code"
    else
            printf "TEST $1 OK!\n"
    fi
}
testx () {
	echo -n $2 '[' $bin $(echo "$3") ']' "=>  "
	$bin $(echo "$3") > "out$1.out"
	$diff "out$1.out" "expect$1.out"
	e_code=$?
    if [ $e_code != 0 ]; then
            printf "TEST $1 FAIL : %d\n" "$e_code"
    else
            printf "TEST $1 OK!\n"
    fi
}

#-P /home/pum/Bork/Projects/Tram/v2021/prog0.trm -T /home/pum/Bork/Projects/Tram/v2021/term0.trm -i -r -O
Test=0
Doc="Most simple test"
cat <<-'******************' > "prog$Test.trm"
	a=b;
******************
cat <<-'******************' > "term$Test.trm"
	f(a,#100)
******************
cat <<-'******************' > "expect$Test.out"
	f(b,#0x64)
******************
test "$Test" "$Doc"

Test=10
Doc="Check data scanner and printer"
cat <<-'******************' > "prog$Test.trm"
	a=b;
******************
cat <<-'******************' > "term$Test.trm"
	!comment
	test(eos,str('a',eos),eol,lst('a',eol),#12345,#0,#1,#007,#0xDedBef,'A',#-1,*A,$A1,antroposofie,Xantippe,Y,a)
******************
cat <<-'******************' > "expect$Test.out"
	test(eos,str(#0x61,eos),eol,lst(#0x61,eol),#0x3039,#0x0,#0x1,#0x7,#0xdedbef,#0x41,#0x3fffffff,*A,$A1,antro...,Xant...,Y,b)
******************
test "$Test" "$Doc"

Test=20
Doc="Check rewriter"
cat <<-'******************' > "prog$Test.trm"
	p(s(X),Y) = s(p(X,Y));
	p(z,X) = X;
	t(s(X),Y) = p(t(X,Y),Y);
	t(z,X) = z;
******************
cat <<-'******************' > "term$Test.trm"
	t(s(s(s(s(s(s(t(s(s(s(s(s(s(z)))))),s(s(s(s(s(z)))))))))))),s(s(s(s(s(t(s(s(s(s(s(s(z)))))),s(s(s(s(s(z))))))))))))
******************
cat <<-'******************' > "expect$Test.out"
	s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(s(z))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))))
******************
test "$Test" "$Doc"


Test=30
Flags="-p prog1$Test.trm -p prog2$Test.trm -C -I -T term$Test.trm -r -O"
Doc="Check combine sources"
cat <<-'******************' > "prog1$Test.trm"
	a(s(X),p(Y)) = a(X,Y);
	a(s(X),Y) = s(a(X,Y));
******************
cat <<-'******************' > "prog2$Test.trm"
	a(X,p(Y)) = p(a(X,Y));
	a(z,X) = X;
	a(X,Y) = a(Y,X);
******************
cat <<-'******************' > "term$Test.trm"
	a(p(p(p(z))),s(s(s(s(s(z))))))
******************
cat <<-'******************' > "expect$Test.out"
	a(s(X),p(Y)) = a(X,Y);
	a(s(X),Y) = s(a(X,Y));
	a(X,p(Y)) = p(a(X,Y));
	a(z,X) = X;
	a(X,Y) = a(Y,X);


	s(s(z))
******************
testx "$Test" "$Doc" "$Flags"


Test=40
Flags="-P prog$Test.trm -I -t term2$Test.trm -t term3$Test.trm -M term1$Test.trm -i -r -O"
Doc="Check combine terms with template"
cat <<-'******************' > "prog$Test.trm"
	a(s(X),p(Y)) = a(X,Y);
	a(s(X),Y) = s(a(X,Y));
	a(X,p(Y)) = p(a(X,Y));
	a(z,X) = X;
	a(X,Y) = a(Y,X);
******************
cat <<-'******************' > "term1$Test.trm"
	a(p(%1),s(s(s(%2))))
******************
cat <<-'******************' > "term2$Test.trm"
	p(p(z))
******************
cat <<-'******************' > "term3$Test.trm"
	s(s(z))
******************
cat <<-'******************' > "expect$Test.out"
	a(s(X),p(Y)) = a(X,Y);
	a(s(X),Y) = s(a(X,Y));
	a(X,p(Y)) = p(a(X,Y));
	a(z,X) = X;
	a(X,Y) = a(Y,X);
	
	
	a(p(p(p(z))),s(s(s(s(s(z))))))
	s(s(z))
******************
testx "$Test" "$Doc" "$Flags"

#-P /home/pum/Bork/Projects/Tram/v2021/prog50.trm -s /home/pum/Bork/Projects/Tram/v2021/term250.trm -M /home/pum/Bork/Projects/Tram/v2021/term150.trm -r -O
Test=50
Flags="-P prog$Test.trm -I -s term2$Test.trm -M term1$Test.trm -i -r -O"
Doc="Check combine sources"
cat <<-'******************' > "prog$Test.trm"
	rev(S) = revx(S,eos);
	revx(eos,S) = S;
	revx(str(C,S),T) = revx(S,str(C,T));
******************
cat <<-'******************' > "term1$Test.trm"
	rev(%1)
******************
cat <<-'******************' > "term2$Test.trm"
	xyz
******************
truncate -s -1 "term2$Test.trm" #get rid of final newline
cat <<-'******************' > "expect$Test.out"
	rev(S) = revx(S,eos);
	revx(eos,S) = S;
	revx(str(C,S),T) = revx(S,str(C,T));


	rev(str(#0x78,str(#0x79,str(#0x7a,eos))))
	str(#0x7a,str(#0x79,str(#0x78,eos)))
******************
testx "$Test" "$Doc" "$Flags"


cat <<-'******************' > "BinaryNumbers.trm"
	bin(#0,X) = X;
	bin(X,bin(Y,Z)) = bin(add(X,Y),Z);

	eq(bin(N,M),bin(P,Q)) = eqn(eq(N,P),M,Q);

	add(#1,#1) = bin(#1,#0);
	add(#0,X) = X;
	add(X,#0) = X;
	add(X,bin(Y,Z)) = bin(Y,add(X,Z));
	add(bin(X,Y),Z) = bin(X,add(Y,Z));

	mul(#0,N) = #0;      mul(N,#0) = #0;
	mul(#1,N) = N;      mul(N,#1) = N;
	mul(bin(A,B),bin(C,D))
	   = bin(bin(mul(A,C),add(mul(A,D),mul(B,C))),mul(B,D));

	succ(N) = add(N,#1);
******************

Test=100
Flags="-P BinaryNumbers.trm -T term$Test.trm -r -O"
Doc="Scan Numbers 0, 1"
cat <<-'******************' > "term$Test.trm"
	mul(bin(bin(#1,#0),#1),bin(bin(#1,#1),#0))
******************
cat <<-'******************' > "expect$Test.out"
	bin(bin(bin(bin(#0x1,#0x1),#0x1),#0x1),#0x0)
******************
testx "$Test" "$Doc" "$Flags"


cat <<-'******************' > "HexConvert.trm"
	shftr(bin(bin(bin(bin(B,B3),B2),B1),B0)) = B;
	shftr(N) = #0;

	shftl(N) = bin(bin(bin(bin(N,#0),#0),#0),#0);

	modF(bin(bin(bin(bin(B,B3),B2),B1),B0)) = bin(bin(bin(B3,B2),B1),B0);
	modF(N) = N;

	hex(N)
	   = cat(quad(shftr(shftr(shftr(shftr(shftr(shftr(shftr(N)))))))),
	     cat(quad(modF(shftr(shftr(shftr(shftr(shftr(shftr(N)))))))),
	     cat(quad(modF(shftr(shftr(shftr(shftr(shftr(N))))))),
	     cat(quad(modF(shftr(shftr(shftr(shftr(N)))))),
	     cat(quad(modF(shftr(shftr(shftr(N))))),
	     cat(quad(modF(shftr(shftr(N)))),
	     cat(quad(modF(shftr(N))),
	     cat(quad(modF(N)),eos))))))));

	quad(#0) = '0';
	quad(#1) = '1';
	quad(bin(#1,#0)) = '2';
	quad(bin(#1,#1)) = '3';
	quad(bin(bin(#1,#0),#0)) = '4';
	quad(bin(bin(#1,#0),#1)) = '5';
	quad(bin(bin(#1,#1),#0)) = '6';
	quad(bin(bin(#1,#1),#1)) = '7';
	quad(bin(bin(bin(#1,#0),#0),#0)) = '8';
	quad(bin(bin(bin(#1,#0),#0),#1)) = '9';
	quad(bin(bin(bin(#1,#0),#1),#0)) = 'a';
	quad(bin(bin(bin(#1,#0),#1),#1)) = 'b';
	quad(bin(bin(bin(#1,#1),#0),#0)) = 'c';
	quad(bin(bin(bin(#1,#1),#0),#1)) = 'd';
	quad(bin(bin(bin(#1,#1),#1),#0)) = 'e';
	quad(bin(bin(bin(#1,#1),#1),#1)) = 'f';
******************

Test=110
Flags="-p BinaryNumbers.trm -p HexConvert.trm -C -T term$Test.trm -r -O"
Doc="Hex convert; larger terms"
cat <<-'******************' > "term$Test.trm"
	hex(bin(bin(bin(bin(#0x1,#0x1),#0x1),#0x1),#0x0))
******************
cat <<-'******************' > "expect$Test.out"
	cat(#0x30,cat(#0x30,cat(#0x30,cat(#0x30,cat(#0x30,cat(#0x30,cat(#0x31,cat(#0x65,eos))))))))
******************
#output means bin 11110 => hex 0000001e, Cat isn't reduced 'cos Strings.trm not yet included
testx "$Test" "$Doc" "$Flags"


# Some Basics
cat <<-'******************' > "Lists.trm"
	append(C,eos) = str(C,eos);
	append(B,str(C,S)) = cat(C,append(B,S));
	append(X,eol) = lst(X,eol);
	append(Y,lst(X,L)) = lst(X,append(Y,L));
	append(X,eoa) = arg(X,eoa);
	append(Y,arg(X,Xs)) = arg(X,append(Y,Xs));
	append(X,trm(F,Xs)) = trm(F,append(X,Xs));

	first(lst(X,L)) = X;
	first(str(C,S)) = C;

	rest(lst(X,L)) = L;
	rest(str(C,S)) = S;
******************
cat <<-'******************' > "Strings.trm"
	cat(str(C,S),T) = str(C,cat(S,T));
	cat(eos,T) = T;
	cat(C,T) = str(C,T);

	eq(str(C1,S1),str(C2,S2)) = eqn(eq(C1,C2),S1,S2);
	eq(eos,eos) = ok;

	eqn(ok,S1,S2) = eq(S1,S2);

	reverse(eos) = eos;
	reverse(str(C,S)) = reversex(C,S,eos);

	reversex(C,eos,S) = str(C,S);
	reversex(C,str(B,S),T) = reversex(B,S,str(C,T));

	eq('a','a') = ok;  eq('b','b') = ok;  eq('c','c') = ok;  eq('d','d') = ok;
	eq('e','e') = ok;  eq('f','f') = ok;  eq('g','g') = ok;  eq('h','h') = ok;
	eq('i','i') = ok;  eq('j','j') = ok;  eq('k','k') = ok;  eq('l','l') = ok;
	eq('m','m') = ok;  eq('n','n') = ok;  eq('o','o') = ok;  eq('p','p') = ok;
	eq('q','q') = ok;  eq('r','r') = ok;  eq('s','s') = ok;  eq('t','t') = ok;
	eq('u','u') = ok;  eq('v','v') = ok;  eq('w','w') = ok;  eq('x','x') = ok;
	eq('y','y') = ok;  eq('z','z') = ok;
	eq('A','A') = ok;  eq('B','B') = ok;  eq('C','C') = ok;  eq('D','D') = ok;
	eq('E','E') = ok;  eq('F','F') = ok;  eq('G','G') = ok;  eq('H','H') = ok;
	eq('I','I') = ok;  eq('J','J') = ok;  eq('K','K') = ok;  eq('L','L') = ok;
	eq('M','M') = ok;  eq('N','N') = ok;  eq('O','O') = ok;  eq('P','P') = ok;
	eq('Q','Q') = ok;  eq('R','R') = ok;  eq('S','S') = ok;  eq('T','T') = ok;
	eq('U','U') = ok;  eq('V','V') = ok;  eq('W','W') = ok;  eq('X','X') = ok;
	eq('Y','Y') = ok;  eq('Z','Z') = ok;
	eq('0','0') = ok;  eq('1','1') = ok;  eq('2','2') = ok;  eq('3','3') = ok;
	eq('4','4') = ok;  eq('5','5') = ok;  eq('6','6') = ok;  eq('7','7') = ok;
	eq('8','8') = ok;  eq('9','9') = ok;
	eq('!','!') = ok;  eq('"','"') = ok;  eq('#','#') = ok;  eq('(','(') = ok;
	eq(')',')') = ok;  eq(',',',') = ok;  eq('.','.') = ok;  eq(';',';') = ok;
	eq('=','=') = ok;  eq('^','^') = ok;
	eq(#0x20,#0x20)= ok; eq(#0x0A,#0x0A)= ok; eq(#0x21,#0x21)= ok; eq(#0x0D,#0x0D)= ok;
	eq(#09,#09) = ok;

	aschr(bin(bin(bin(#1,#0),#0),#1)) = #0x09;
	aschr(bin(bin(bin(#1,#0),#1),#0)) = #0x0A;
	aschr(bin(bin(bin(#1,#1),#0),#1)) = #0x0D;
	aschr(bin(bin(bin(bin(bin(#1,#0),#0),#0),#0),#0)) = #0x20;

	aschr(bin(bin(bin(bin(bin(#1,#0),#0),#0),#0),#1)) = '!';
	aschr(bin(bin(bin(bin(bin(#1,#0),#0),#0),#1),#0)) = '"';
	aschr(bin(bin(bin(bin(bin(#1,#0),#0),#0),#1),#1)) = '#';
	aschr(bin(bin(bin(bin(bin(#1,#0),#1),#0),#0),#0)) = '(';
	aschr(bin(bin(bin(bin(bin(#1,#0),#1),#0),#0),#1)) = ')';
	aschr(bin(bin(bin(bin(bin(#1,#0),#1),#1),#0),#0)) = ',';
	aschr(bin(bin(bin(bin(bin(#1,#0),#1),#1),#1),#0)) = '.';
	aschr(bin(bin(bin(bin(bin(#1,#1),#1),#0),#1),#0)) = ':';
	aschr(bin(bin(bin(bin(bin(#1,#1),#1),#0),#1),#1)) = ';';
	aschr(bin(bin(bin(bin(bin(#1,#1),#1),#1),#0),#1)) = '=';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#1),#1),#1),#1),#0)) = '^';

	aschr(bin(bin(bin(bin(bin(#1,#1),#0),#0),#0),#0)) = '0';
	aschr(bin(bin(bin(bin(bin(#1,#1),#0),#0),#0),#1)) = '1';
	aschr(bin(bin(bin(bin(bin(#1,#1),#0),#0),#1),#0)) = '2';
	aschr(bin(bin(bin(bin(bin(#1,#1),#0),#0),#1),#1)) = '3';
	aschr(bin(bin(bin(bin(bin(#1,#1),#0),#1),#0),#0)) = '4';
	aschr(bin(bin(bin(bin(bin(#1,#1),#0),#1),#0),#1)) = '5';
	aschr(bin(bin(bin(bin(bin(#1,#1),#0),#1),#1),#0)) = '6';
	aschr(bin(bin(bin(bin(bin(#1,#1),#0),#1),#1),#1)) = '7';
	aschr(bin(bin(bin(bin(bin(#1,#1),#1),#0),#0),#0)) = '8';
	aschr(bin(bin(bin(bin(bin(#1,#1),#1),#0),#0),#1)) = '9';

	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#0),#0),#0),#0),#1)) = 'a';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#0),#0),#0),#1),#0)) = 'b';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#0),#0),#0),#1),#1)) = 'c';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#0),#0),#1),#0),#0)) = 'd';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#0),#0),#1),#0),#1)) = 'e';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#0),#0),#1),#1),#0)) = 'f';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#0),#0),#1),#1),#1)) = 'g';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#0),#1),#0),#0),#0)) = 'h';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#0),#1),#0),#0),#1)) = 'i';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#0),#1),#0),#1),#0)) = 'j';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#0),#1),#0),#1),#1)) = 'k';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#0),#1),#1),#0),#0)) = 'l';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#0),#1),#1),#0),#1)) = 'm';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#0),#1),#1),#1),#0)) = 'n';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#0),#1),#1),#1),#1)) = 'o';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#1),#0),#0),#0),#0)) = 'p';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#1),#0),#0),#0),#1)) = 'q';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#1),#0),#0),#1),#0)) = 'r';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#1),#0),#0),#1),#1)) = 's';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#1),#0),#1),#0),#0)) = 't';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#1),#0),#1),#0),#1)) = 'u';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#1),#0),#1),#1),#0)) = 'v';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#1),#0),#1),#1),#1)) = 'w';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#1),#1),#0),#0),#0)) = 'x';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#1),#1),#0),#0),#1)) = 'y';
	aschr(bin(bin(bin(bin(bin(bin(#1,#1),#1),#1),#0),#1),#0)) = 'z';

	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#0),#0),#0),#0),#1)) = 'A';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#0),#0),#0),#1),#0)) = 'B';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#0),#0),#0),#1),#1)) = 'C';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#0),#0),#1),#0),#0)) = 'D';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#0),#0),#1),#0),#1)) = 'E';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#0),#0),#1),#1),#0)) = 'F';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#0),#0),#1),#1),#1)) = 'G';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#0),#1),#0),#0),#0)) = 'H';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#0),#1),#0),#0),#1)) = 'I';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#0),#1),#0),#1),#0)) = 'J';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#0),#1),#0),#1),#1)) = 'K';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#0),#1),#1),#0),#0)) = 'L';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#0),#1),#1),#0),#1)) = 'M';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#0),#1),#1),#1),#0)) = 'N';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#0),#1),#1),#1),#1)) = 'O';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#1),#0),#0),#0),#0)) = 'P';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#1),#0),#0),#0),#1)) = 'Q';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#1),#0),#0),#1),#0)) = 'R';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#1),#0),#0),#1),#1)) = 'S';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#1),#0),#1),#0),#0)) = 'T';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#1),#0),#1),#0),#1)) = 'U';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#1),#0),#1),#1),#0)) = 'V';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#1),#0),#1),#1),#1)) = 'W';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#1),#1),#0),#0),#0)) = 'X';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#1),#1),#0),#0),#1)) = 'Y';
	aschr(bin(bin(bin(bin(bin(bin(#1,#0),#1),#1),#0),#1),#0)) = 'Z';

	ascii(#0x09) = bin(bin(bin(bin(bin(#0,#0),#1),#0),#0),#1);
	ascii(#0x0A) = bin(bin(bin(bin(bin(#0,#0),#1),#0),#1),#0);
	ascii(#0x0D) = bin(bin(bin(bin(bin(#0,#0),#1),#1),#0),#1);
	ascii(#0x20) = bin(bin(bin(bin(bin(#1,#0),#0),#0),#0),#0);

	ascii('!') = bin(bin(bin(bin(bin(#1,#0),#0),#0),#0),#1);
	ascii('"') = bin(bin(bin(bin(bin(#1,#0),#0),#0),#1),#0);
	ascii('#') = bin(bin(bin(bin(bin(#1,#0),#0),#0),#1),#1);
	ascii('(') = bin(bin(bin(bin(bin(#1,#0),#1),#0),#0),#0);
	ascii(')') = bin(bin(bin(bin(bin(#1,#0),#1),#0),#0),#1);
	ascii(',') = bin(bin(bin(bin(bin(#1,#0),#1),#1),#0),#0);
	ascii('.') = bin(bin(bin(bin(bin(#1,#0),#1),#1),#1),#0);
	ascii(':') = bin(bin(bin(bin(bin(#1,#1),#1),#0),#1),#0);
	ascii(';') = bin(bin(bin(bin(bin(#1,#1),#1),#0),#1),#1);
	ascii('=') = bin(bin(bin(bin(bin(#1,#1),#1),#1),#0),#1);
	ascii('^') = bin(bin(bin(bin(bin(bin(#1,#1),#1),#1),#1),#1),#0);

	ascii('0') = bin(bin(bin(bin(bin(bin(#0,#1),#1),#0),#0),#0),#0);
	ascii('1') = bin(bin(bin(bin(bin(bin(#0,#1),#1),#0),#0),#0),#1);
	ascii('2') = bin(bin(bin(bin(bin(bin(#0,#1),#1),#0),#0),#1),#0);
	ascii('3') = bin(bin(bin(bin(bin(bin(#0,#1),#1),#0),#0),#1),#1);
	ascii('4') = bin(bin(bin(bin(bin(bin(#0,#1),#1),#0),#1),#0),#0);
	ascii('5') = bin(bin(bin(bin(bin(bin(#0,#1),#1),#0),#1),#0),#1);
	ascii('6') = bin(bin(bin(bin(bin(bin(#0,#1),#1),#0),#1),#1),#0);
	ascii('7') = bin(bin(bin(bin(bin(bin(#0,#1),#1),#0),#1),#1),#1);
	ascii('8') = bin(bin(bin(bin(bin(bin(#0,#1),#1),#1),#0),#0),#0);
	ascii('9') = bin(bin(bin(bin(bin(bin(#0,#1),#1),#1),#0),#0),#1);

	ascii('a') = bin(bin(bin(bin(bin(bin(#1,#1),#0),#0),#0),#0),#1);
	ascii('b') = bin(bin(bin(bin(bin(bin(#1,#1),#0),#0),#0),#1),#0);
	ascii('c') = bin(bin(bin(bin(bin(bin(#1,#1),#0),#0),#0),#1),#1);
	ascii('d') = bin(bin(bin(bin(bin(bin(#1,#1),#0),#0),#1),#0),#0);
	ascii('e') = bin(bin(bin(bin(bin(bin(#1,#1),#0),#0),#1),#0),#1);
	ascii('f') = bin(bin(bin(bin(bin(bin(#1,#1),#0),#0),#1),#1),#0);
	ascii('g') = bin(bin(bin(bin(bin(bin(#1,#1),#0),#0),#1),#1),#1);
	ascii('h') = bin(bin(bin(bin(bin(bin(#1,#1),#0),#1),#0),#0),#0);
	ascii('i') = bin(bin(bin(bin(bin(bin(#1,#1),#0),#1),#0),#0),#1);
	ascii('j') = bin(bin(bin(bin(bin(bin(#1,#1),#0),#1),#0),#1),#0);
	ascii('k') = bin(bin(bin(bin(bin(bin(#1,#1),#0),#1),#0),#1),#1);
	ascii('l') = bin(bin(bin(bin(bin(bin(#1,#1),#0),#1),#1),#0),#0);
	ascii('m') = bin(bin(bin(bin(bin(bin(#1,#1),#0),#1),#1),#0),#1);
	ascii('n') = bin(bin(bin(bin(bin(bin(#1,#1),#0),#1),#1),#1),#0);
	ascii('o') = bin(bin(bin(bin(bin(bin(#1,#1),#0),#1),#1),#1),#1);
	ascii('p') = bin(bin(bin(bin(bin(bin(#1,#1),#1),#0),#0),#0),#0);
	ascii('q') = bin(bin(bin(bin(bin(bin(#1,#1),#1),#0),#0),#0),#1);
	ascii('r') = bin(bin(bin(bin(bin(bin(#1,#1),#1),#0),#0),#1),#0);
	ascii('s') = bin(bin(bin(bin(bin(bin(#1,#1),#1),#0),#0),#1),#1);
	ascii('t') = bin(bin(bin(bin(bin(bin(#1,#1),#1),#0),#1),#0),#0);
	ascii('u') = bin(bin(bin(bin(bin(bin(#1,#1),#1),#0),#1),#0),#1);
	ascii('v') = bin(bin(bin(bin(bin(bin(#1,#1),#1),#0),#1),#1),#0);
	ascii('w') = bin(bin(bin(bin(bin(bin(#1,#1),#1),#0),#1),#1),#1);
	ascii('x') = bin(bin(bin(bin(bin(bin(#1,#1),#1),#1),#0),#0),#0);
	ascii('y') = bin(bin(bin(bin(bin(bin(#1,#1),#1),#1),#0),#0),#1);
	ascii('z') = bin(bin(bin(bin(bin(bin(#1,#1),#1),#1),#0),#1),#0);

	ascii('A') = bin(bin(bin(bin(bin(bin(#1,#0),#0),#0),#0),#0),#1);
	ascii('B') = bin(bin(bin(bin(bin(bin(#1,#0),#0),#0),#0),#1),#0);
	ascii('C') = bin(bin(bin(bin(bin(bin(#1,#0),#0),#0),#0),#1),#1);
	ascii('D') = bin(bin(bin(bin(bin(bin(#1,#0),#0),#0),#1),#0),#0);
	ascii('E') = bin(bin(bin(bin(bin(bin(#1,#0),#0),#0),#1),#0),#1);
	ascii('F') = bin(bin(bin(bin(bin(bin(#1,#0),#0),#0),#1),#1),#0);
	ascii('G') = bin(bin(bin(bin(bin(bin(#1,#0),#0),#0),#1),#1),#1);
	ascii('H') = bin(bin(bin(bin(bin(bin(#1,#0),#0),#1),#0),#0),#0);
	ascii('I') = bin(bin(bin(bin(bin(bin(#1,#0),#0),#1),#0),#0),#1);
	ascii('J') = bin(bin(bin(bin(bin(bin(#1,#0),#0),#1),#0),#1),#0);
	ascii('K') = bin(bin(bin(bin(bin(bin(#1,#0),#0),#1),#0),#1),#1);
	ascii('L') = bin(bin(bin(bin(bin(bin(#1,#0),#0),#1),#1),#0),#0);
	ascii('M') = bin(bin(bin(bin(bin(bin(#1,#0),#0),#1),#1),#0),#1);
	ascii('N') = bin(bin(bin(bin(bin(bin(#1,#0),#0),#1),#1),#1),#0);
	ascii('O') = bin(bin(bin(bin(bin(bin(#1,#0),#0),#1),#1),#1),#1);
	ascii('P') = bin(bin(bin(bin(bin(bin(#1,#0),#1),#0),#0),#0),#0);
	ascii('Q') = bin(bin(bin(bin(bin(bin(#1,#0),#1),#0),#0),#0),#1);
	ascii('R') = bin(bin(bin(bin(bin(bin(#1,#0),#1),#0),#0),#1),#0);
	ascii('S') = bin(bin(bin(bin(bin(bin(#1,#0),#1),#0),#0),#1),#1);
	ascii('T') = bin(bin(bin(bin(bin(bin(#1,#0),#1),#0),#1),#0),#0);
	ascii('U') = bin(bin(bin(bin(bin(bin(#1,#0),#1),#0),#1),#0),#1);
	ascii('V') = bin(bin(bin(bin(bin(bin(#1,#0),#1),#0),#1),#1),#0);
	ascii('W') = bin(bin(bin(bin(bin(bin(#1,#0),#1),#0),#1),#1),#1);
	ascii('X') = bin(bin(bin(bin(bin(bin(#1,#0),#1),#1),#0),#0),#0);
	ascii('Y') = bin(bin(bin(bin(bin(bin(#1,#0),#1),#1),#0),#0),#1);
	ascii('Z') = bin(bin(bin(bin(bin(bin(#1,#0),#1),#1),#0),#1),#0);
******************


Test=120
Flags="-p Lists.trm -p BinaryNumbers.trm -p Strings.trm -C -T term$Test.trm -r -O"
Doc="Hex convert; larger terms"
cat <<-'******************' > "term$Test.trm"
	first(lst(aschr(add(ascii('a'),#1)),eol))
******************
cat <<-'******************' > "expect$Test.out"
	#0x62
******************
#output means bin 11110 => hex 0000001e, Cat isn't reduced 'cos Strings.trm not yet included
testx "$Test" "$Doc" "$Flags"



# Define scanner
cat <<-'******************' > "ScannerParser.trm"
	scan(str(C,S)) = sccc(cc(C),C,S,null,eol);
	
	sccc(ws,C,S,V,Stck) = sccc(cc(first(S)),first(S),rest(S),V,Stck);
	sccc(comm,C,S,V,Stck) = sccomm(S,V,Stck);
	sccc(spec,C,S,V,Stck) = scspec(C,S,Stck);
	sccc(cap,C,str(B,S),V,Stck) = scsym(cc(B),B,S,cap,str(C,eos),Stck);
	sccc(low,C,str(B,S),V,Stck) = scsym(cc(B),B,S,low,str(C,eos),Stck);
	! sccc(quot,C,S,V,Stck) = scstring(first(S),rest(S),eos,Stck);
	sccc(lpar,C,S,V,Stck)
	    = sccc(cc(first(S)),first(S),rest(S),null,lst(trm(V,eoa),Stck));
	sccc(cc(first(eos)),C,S,V,Stck) = scfinalize(C,S,V,Stck);
	!sccc(X,first(eos),Y,V,Z) = V;
	sccc(CC,C,S,V,Stck) = scaffix(CC,S,V,Stck);
	
	! V is var(), trm(), str(), #C
	
	! scaffix(colon,S,F,Stck) = sclini(equ,S,lst(trm(F,eoa),Stck));
	!scaffix(equ,S,var(V),Stck)
	!= sclini(equ,S,lst(trm(str('r',str('l',eos)),arg(var(V),eoa)),Stck));
	!scaffix(equ,S,str(F,Fs),Stck)
	!= sclini(equ,S,lst(trm(str('r',str('l',eos)),
	!                       arg(trm(str(F,Fs),eoa),eoa)),Stck));
	scaffix(equ,S,trm(F,As),Stck)
	= sclini(equ,S,lst(trm(str('r',str('l',eos)),arg(trm(F,As),eoa)),Stck));
	scaffix(equ,S,F,Stck)
	= sclini(equ,S,lst(trm(str('r',str('l',eos)),arg(trm(F,eoa),eoa)),Stck));
	scaffix(Cc,S,var(V),lst(T,Stck))
	= sclini(Cc,S,lst(append(var(V),T),Stck));
	scaffix(Cc,S,str(F,Fs),lst(T,Stck))
	= sclini(Cc,S,lst(append(trm(str(F,Fs),eoa),T),Stck));
	scaffix(Cc,S,trm(F,As),lst(T,Stck))
	= sclini(Cc,S,lst(append(trm(F,As),T),Stck));
	scaffix(Cc,S,F,lst(T,Stck))
	= sclini(Cc,S,lst(append(trm(F,eoa),T),Stck));
	
	sclini(rpar,S,lst(T,Stck)) = sccc(cc(first(S)),first(S),rest(S),T,Stck);
	sclini(Cc,S,Stck) = sccc(cc(first(S)),first(S),rest(S),null,Stck);
	
	scfinalize(first(eos),rest(eos),trm(F,As),eol) = trm(F,As);
	scfinalize(first(eos),rest(eos),F,eol) = trm(F,eoa);
	scfinalize(first(eos),rest(eos),null,Stck) = sccollrls(trm(str('e',str('o',str('r',eos))),eoa),Stck);
	
	sccollrls(Rs,lst(trm(str('r',str('l',eos)),arg(L,arg(R,eoa))),Stck))
	 = sccollrls(trm(str('r',str('l',eos)),arg(L,arg(R,arg(Rs,eoa)))),Stck);
	sccollrls(V,eol) = V;
	
	scsym(low,C,str(B,S),CC,V,Stck) = scsym(cc(B),B,S,CC,cat(C,V),Stck);
	scsym(cap,C,str(B,S),CC,V,Stck) = scsym(cc(B),B,S,CC,cat(C,V),Stck);
	scsym(num,C,str(B,S),CC,V,Stck) = scsym(cc(B),B,S,CC,cat(C,V),Stck);
	scsym(X,C,S,low,V,Stck) = sccc(X,C,S,reverse(V),Stck);
	scsym(X,C,S,cap,V,Stck) = sccc(X,C,S,var(reverse(V)),Stck);
	
	scspec('#',str('0',str('x',S)),Stck) = schx(S,eos,Stck);
	scspec('#',str('0',S),Stck) = scdec(S,str('0',eos),Stck);
	scspec('#',str('-',S),Stck) = scdec(S,str('-',eos),Stck);
	scspec('#',S,Stck) = scdec(S,eos,Stck);
	scspec(#0x27,str(C,str(#0x27,S)),Stck) = sccc(cc(first(S)),first(S),rest(S),C,Stck);

	sccomm(str(#0xA,S),V,Stck) = sccc(cc(first(S)),first(S),rest(S),V,Stck);
	sccomm(str(C,S),V,Stck) = sccomm(S,V,Stck);
	
	
	! scspec('#',str(C,str(D,S)),Stck) = sccc(cc(first(S)),first(S),rest(S),
	!                                       aschr(add(shftl(unhex(C)),unhex(D))),Stck);
	
	! scstring('',str(C,S),V,Stck) = sccc(cc(C),C,S,reverse(V),Stck);
	scstring(C,str(B,S),V,Stck) = scstring(B,S,cat(C,V),Stck);
	
	scdec(str('0',S),V,Stck) = scdec(S,str('0',V),Stck);
	scdec(str('1',S),V,Stck) = scdec(S,str('1',V),Stck);
	scdec(str('2',S),V,Stck) = scdec(S,str('2',V),Stck);
	scdec(str('3',S),V,Stck) = scdec(S,str('3',V),Stck);
	scdec(str('4',S),V,Stck) = scdec(S,str('4',V),Stck);
	scdec(str('5',S),V,Stck) = scdec(S,str('5',V),Stck);
	scdec(str('6',S),V,Stck) = scdec(S,str('6',V),Stck);
	scdec(str('7',S),V,Stck) = scdec(S,str('7',V),Stck);
	scdec(str('8',S),V,Stck) = scdec(S,str('8',V),Stck);
	scdec(str('9',S),V,Stck) = scdec(S,str('9',V),Stck);
	scdec(S,V,Stck) = sccc(cc(first(S)),first(S),rest(S),reverse(V),Stck);
	
	schx(str('a',S),V,Stck) = schx(S,str('A',V),Stck);
	schx(str('b',S),V,Stck) = schx(S,str('B',V),Stck);
	schx(str('c',S),V,Stck) = schx(S,str('C',V),Stck);
	schx(str('d',S),V,Stck) = schx(S,str('D',V),Stck);
	schx(str('e',S),V,Stck) = schx(S,str('E',V),Stck);
	schx(str('f',S),V,Stck) = schx(S,str('F',V),Stck);
	schx(str('A',S),V,Stck) = schx(S,str('A',V),Stck);
	schx(str('B',S),V,Stck) = schx(S,str('B',V),Stck);
	schx(str('C',S),V,Stck) = schx(S,str('C',V),Stck);
	schx(str('D',S),V,Stck) = schx(S,str('D',V),Stck);
	schx(str('E',S),V,Stck) = schx(S,str('E',V),Stck);
	schx(str('F',S),V,Stck) = schx(S,str('F',V),Stck);
	schx(str('0',S),V,Stck) = schx(S,str('0',V),Stck);
	schx(str('1',S),V,Stck) = schx(S,str('1',V),Stck);
	schx(str('2',S),V,Stck) = schx(S,str('2',V),Stck);
	schx(str('3',S),V,Stck) = schx(S,str('3',V),Stck);
	schx(str('4',S),V,Stck) = schx(S,str('4',V),Stck);
	schx(str('5',S),V,Stck) = schx(S,str('5',V),Stck);
	schx(str('6',S),V,Stck) = schx(S,str('6',V),Stck);
	schx(str('7',S),V,Stck) = schx(S,str('7',V),Stck);
	schx(str('8',S),V,Stck) = schx(S,str('8',V),Stck);
	schx(str('9',S),V,Stck) = schx(S,str('9',V),Stck);
	schx(S,V,Stck) = sccc(cc(first(S)),first(S),rest(S),reverse(V),Stck);
	
	cc('*') = low;  cc('&') = low;
	cc('a') = low;  cc('b') = low;  cc('c') = low;  cc('d') = low;  cc('e') = low;
	cc('f') = low;  cc('g') = low;  cc('h') = low;  cc('i') = low;  cc('j') = low;
	cc('k') = low;  cc('l') = low;  cc('m') = low;  cc('n') = low;  cc('o') = low;
	cc('p') = low;  cc('q') = low;  cc('r') = low;  cc('s') = low;  cc('t') = low;
	cc('u') = low;  cc('v') = low;  cc('w') = low;  cc('x') = low;  cc('y') = low;
	cc('z') = low;
	cc('@') = cap;  cc('$') = cap;  
	cc('A') = cap;  cc('B') = cap;  cc('C') = cap;  cc('D') = cap;  cc('E') = cap;
	cc('F') = cap;  cc('G') = cap;  cc('H') = cap;  cc('I') = cap;  cc('J') = cap;
	cc('K') = cap;  cc('L') = cap;  cc('M') = cap;  cc('N') = cap;  cc('O') = cap;
	cc('P') = cap;  cc('Q') = cap;  cc('R') = cap;  cc('S') = cap;  cc('T') = cap;
	cc('U') = cap;  cc('V') = cap;  cc('W') = cap;  cc('X') = cap;  cc('Y') = cap;
	cc('Z') = cap;
	cc('0') = num;  cc('1') = num;  cc('2') = num;  cc('3') = num;  cc('4') = num;
	cc('5') = num;  cc('6') = num;  cc('7') = num;  cc('8') = num;  cc('9') = num;
	cc('(') = lpar; cc(')') = rpar; cc(',') = sepa;
	cc('=') = equ;  cc(';') = sepa; cc(':') = colon;   ! cc('.') = period;
	cc(#0x27) = spec; cc('#') = spec;
	cc('!') = comm; cc('"') = quot;
	cc(#0x20) = ws;  cc(#0xA) = ws;  cc(#0xD) = ws;  cc(#0x9) = ws;
******************


Test=200
Flags="-D 1 -X 2 -p ScannerParser.trm -p Lists.trm -p Strings.trm -C -s term2$Test.trm -M term1$Test.trm -r -O"
Doc="Scanner scan simple term"
cat <<-'******************' > "term1$Test.trm"
	scan(%1)
******************
cat <<-'******************' > "term2$Test.trm"
	p(p(z))
******************
cat <<-'******************' > "expect$Test.out"

	### Start ###
	### Done! ###
	trm("p",arg(trm("p",arg(trm("z",eoa),eoa)),eoa))
******************
testx "$Test" "$Doc" "$Flags"


