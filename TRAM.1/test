# ArityChecker checks if the arity (number of arguments) of a function symbol is consistent (i.e. equal for all uses of that symbol)

cat <<-'******************' > "ArtyChecker.trm"
	aritychk(T) = ac(T,eol,eot,eol);

	ac(trm(F,As),Stck,Tab,Errs) = achk(count(As),get(F,Tab),lst(As,Stck),F,Tab,Errs);
	ac(var(N),Stck,Tab,Errs) = ac(eoa,Stck,Tab,Errs);
	ac(arg(A,As),Stck,Tab,Errs) = ac(As,lst(A,Stck),Tab,Errs);
	ac(eoa,lst(X,Stck),Tab,Errs) = ac(X,Stck,Tab,Errs);
	ac(eoa,eol,Tab,Errs) = Errs;

	achk(N,get(*,eot),Stck,F,Tab,Errs) = ac(eoa,Stck,tab(F,N,Tab),Errs);
	achk(N,M,Stck,F,Tab,Errs) = achkq(eq(N,M),N,Stck,F,Tab,Errs);

	achkq(ok,*,Stck,*,Tab,Errs) = ac(eoa,Stck,Tab,Errs);
	achkq(*,N,Stck,F,Tab,Errs) = ac(eoa,Stck,Tab,lst(errA(F,N),Errs));

	get(F,tab(G,M,Tab)) = getq(eq(F,G),F,M,Tab);

	getq(ok,F,M,Tab) = M;
	getq(*,F,*,Tab) = get(F,Tab);

	count(eoa) = z;
	count(arg(A,As)) = s(count(As));

	eq(z,z) = ok; eq(s(X),s(Y)) = eq(X,Y);

	f(A,B,C) = f(A,B);
******************

Test=510
Flags="-D 1 -X 10 -p ScannerParser.trm -p ArtyChecker.trm -p Lists.trm -p Strings.trm -C -s ArtyChecker.trm -M term1$Test.trm -r -O"
Doc="Scanner scan self"
cat <<-'******************' > "term1$Test.trm"
	aritychk(scan(%1))
******************
cat <<-'******************' > "expect$Test.out"

	### Start ###
	### Done! ###
	lst(errA("f",s(s(s(z)))),eol)
******************
testx "$Test" "$Doc" "$Flags"



